{
  "name": "nuxt-gtag",
  "type": "module",
  "version": "0.3.0",
  "packageManager": "pnpm@7.30.0",
  "description": "Nuxt 3 module to natively integrate Google Analytics",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/nuxt-gtag#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/nuxt-gtag.git"
  },
  "bugs": "https://github.com/johannschopplich/nuxt-gtag/issues",
  "keywords": [
    "nuxt",
    "nuxt3",
    "google",
    "gtag",
    "gtm",
    "analytics"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:types": "tsc --noEmit",
    "release": "bumpp --commit --push --tag",
    "prepare": "nuxi prepare playground"
  },
  "dependencies": {
    "@nuxt/kit": "^3.3.1",
    "defu": "^6.1.2",
    "pathe": "^1.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.37.0",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.3.1",
    "@types/node": "^18.15.5",
    "bumpp": "^9.0.0",
    "eslint": "^8.36.0",
    "nuxt": "^3.3.1",
    "nuxt-gtag": "workspace:*",
    "typescript": "^5.0.2",
    "vue-tsc": "^1.2.0"
  }
}
